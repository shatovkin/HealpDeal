<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dimitri.helpdeal.azureClasses.menuActivities.UserProfile_Activity">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Password:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="75dp"
        android:gravity="right"
        android:text="Firstname:"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/txt_firstname_profile"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_firstname_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="75dp"
        android:text="firstname"
        android:textColor="@color/black"
        app:layout_constraintHorizontal_bias="0.695"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="12dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Name:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/txt_name_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="name_profile"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/txt_firstname_profile" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Password:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/txt_password_confirm_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="password"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/txt_password_profile"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/txt_password_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="password"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/txt_name_profile" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Index:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/txt_index_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:text="index"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_password_confirm_profile" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="City:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:text="City:" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Street:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:text="Phone:"
        android:gravity="right"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="city"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/txt_index_profile" />

    <TextView
        android:id="@+id/txt_street_profile"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="street"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <TextView
        android:id="@+id/txt_phone_profile"
        android:layout_width="173dp"
        android:layout_height="23dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="8dp"
        android:text="phone"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/txt_street_profile" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="132dp"
        android:layout_marginTop="30dp"
        android:text="Profile Data"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/txt_firstname_profile"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>
